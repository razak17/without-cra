{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "rules": {
    "no-shadow": "off",
    "max-len": [
      "warn",
      {
        "code": 100
      }
    ],
    "semi": "warn"
  }
}
